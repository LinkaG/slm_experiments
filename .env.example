# ClearML Configuration
# Установите USE_CLEARML=false для работы без ClearML и MinIO
# В этом режиме все результаты будут сохраняться локально в структурированном формате
USE_CLEARML=false

# ClearML Server Settings (используются только если USE_CLEARML=true)
CLEARML_API_HOST=http://{ip}:8008
CLEARML_WEB_HOST=http://{ip}:8080
CLEARML_FILES_HOST=http://{ip}:8081

# MinIO/S3 Settings (используются только если USE_CLEARML=true)
CLEARML_S3_ENDPOINT=http://{ip}:9000
CLEARML_S3_BUCKET=clearml-artifacts
CLEARML_S3_ACCESS_KEY=minio_admin_2024
CLEARML_S3_SECRET_KEY=placeholder
CLEARML_S3_REGION=us-east-1
CLEARML_S3_PATH_STYLE=true
CLEARML_S3_VERIFY_SSL=false
